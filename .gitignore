# ignore all files starting with . or ~
.*
~*
!.gitignore
!.editorconfig
!README.md
!CHANGELOG.md
!composer.json
!.jshintrc

# ignore node dependency directories (used by grunt)
node_modules/
bower_components/

# ignore OS generated files
ehthumbs.db
Thumbs.db

# ignore Editor files
*.sublime-project
*.sublime-workspace
*.komodoproject
*.codekit

# ignore log files and databases
*.log
*.sql
*.sqlite


# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip
